{
  "name": "@nevermined-io/components-catalog",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false PORT=3445 BROWSER=none react-app-rewired start",
    "build": "npm run clean && npm run build:tsc && npm run copy:scss && npm run build:webpack",
    "build:react": "react-scripts build",
    "build:tsc": "tsc --noEmit false",
    "clean": "rm -rf dist",
    "copy:scss": "cd src/lib && find . -name '*.scss' -exec cp --parents \\{\\} ../../dist/node \\; && cd ../..",
    "test": "react-scripts test --coverage --watchAll=false --silent --env=./test/custom-env.js",
    "eject": "react-scripts eject",
    "ts:lint": "npx eslint . --ext .ts,.tsx",
    "test:e2e": "start-test start http-get://localhost:3000 cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "docs:generate": "typedoc",
    "docs:serve": "serve docs -p 4000",
    "docs:dev": "npm run docs:generate && npm run docs:serve"
  },
  "main": "./dist/index.js",
  "types": "./dist/node/index.d.ts",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@nevermined-io/nevermined-sdk-js": "0.19.19",
    "react-app-rewired": "^2.2.1",
    "@truffle/hdwallet-provider": "^2.0.0",
    "axios": "^0.24.0",
    "bem-helpers": "^1.1.0",
    "stream-browserify": "^3.0.0",
    "node-fetch": "^3.2.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "stream-http": "^3.2.0",
    "bip39": "^3.0.4",
    "classnames": "^2.3.1",
    "lodash.uniqby": "^4.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-hook-form": "7.22.0",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.2",
    "web3": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@metamask/providers": "^8.1.1",
    "@react-mock/state": "^0.1.8",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^12.1.10",
    "@types/cypress": "^1.1.3",
    "@types/jest": "^27.0.3",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "babel-loader": "8.1.0",
    "css-loader": "5.2.7",
    "cypress": "^9.1.1",
    "cypress-log-to-output": "^1.1.2",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "jest-environment-jsdom": "^27.4.4",
    "jest-mock-axios": "^4.5.0",
    "mini-css-extract-plugin": "0.9.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "prettier-stylelint": "^0.4.2",
    "sass": "^1.45.0",
    "sass-loader": "10.1.1",
    "serve": "^13.0.2",
    "start-server-and-test": "^1.14.0",
    "style-loader": "2.0.0",
    "typedoc": "^0.22.10",
    "typedoc-custom-theme-demo": "^1.0.0",
    "typescript": "^4.5.4"
  },
  "resolve": {
    "fallback": {
      "https": "false",
      "url": "false",
      "http": "false"
    }
  }
}
